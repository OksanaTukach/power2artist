                    +inputText('Имя артиста', 'Например: Beyonce') 
                    +select('Тип артиста', 'Сольник') 
                    +select('Жанр', 'Pop') 
                    +select('Страна', 'Беларусь') 
                    +select('Город', 'Минск') 
                    +select('Регионы в которых готовы работать', 'Минская область') 
